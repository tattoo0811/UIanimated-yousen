{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ios:clean": "xcodebuild -workspace ios/mobile.xcworkspace -scheme mobile clean",
    "ios:build": "xcodebuild -workspace ios/mobile.xcworkspace -scheme mobile -sdk iphoneos -destination 'generic/platform=iOS' -configuration Debug build",
    "ios:archive": "xcodebuild -workspace ios/mobile.xcworkspace -scheme mobile -sdk iphoneos -configuration Release archive -archivePath build/mobile.xcarchive",
    "ios:export": "xcodebuild -exportArchive -archivePath build/mobile.xcarchive -exportPath build/ipa -exportOptionsPlist ios/ExportOptions.plist"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-firebase/analytics": "^23.7.0",
    "@react-native-firebase/app": "^23.7.0",
    "@react-native-firebase/crashlytics": "^23.7.0",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/native": "^7.1.8",
    "astronomia": "^4.1.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.30",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "js-yaml": "^4.1.1",
    "lucide-react-native": "^0.562.0",
    "lunar-javascript": "^1.6.12",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "^8.0.0",
    "react-native-purchases": "^9.6.13",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "~19.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-expo": "^54.0.16",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
