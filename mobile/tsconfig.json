{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@/src/*": ["./src/*"],
      "@/components/*": ["./components/*", "./src/components/*"],
      "@/data/*": ["./data/*", "./src/data/*"],
      "@/lib/*": ["./lib/*", "./src/lib/*"],
      "@/hooks/*": ["./hooks/*", "./src/hooks/*"],
      "@/contexts/*": ["./contexts/*", "./src/contexts/*"],
      "@/themes/*": ["./themes/*", "./src/themes/*"],
      "@/types/*": ["./types/*", "./src/types/*"],
      "@/utils/*": ["./utils/*", "./src/utils/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}